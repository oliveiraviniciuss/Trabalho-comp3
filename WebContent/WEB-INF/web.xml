<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Sistema Museu Comp3</display-name>
  <servlet>
    <servlet-name>Solicitar Criacao Museu</servlet-name>
    <servlet-class>SolicitarCriacaoMuseuServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>Solicitar Criacao Museu</servlet-name>
      <url-pattern>/solicitarcriacaomuseuSvlt</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Criar Usuario</servlet-name>
    <servlet-class>CriarUsuarioServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>Criar Usuario</servlet-name>
      <url-pattern>/criarusuarioSvlt</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>Criar Gestor</servlet-name>
    <servlet-class>CriarGestorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>Criar Gestor</servlet-name>
      <url-pattern>/criargestorSvlt</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>Criar Museu</servlet-name>
    <servlet-class>CriarMuseuServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>Criar Museu</servlet-name>
      <url-pattern>/criarmuseuSvlt</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>    
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>